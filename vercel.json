{
  "version": 2,
  "framework": null,
  "functions": {
    "api/feed.ts": {
      "runtime": "edge",
      "regions": ["fra1", "cdg1", "arn1"],
      "memory": 128
    }
  },
  "crons": [
    { "path": "/api/feed?mode=refresh-inventory", "schedule": "*/15 * * * *" },
    { "path": "/api/feed?mode=refresh-content", "schedule": "0 4 * * *" }
  ],
  "rewrites": [
    { "source": "/api/feed", "destination": "/api/feed" }
  ]
}
